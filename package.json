        "url": "https://github.com/konstantin-hatvan/traceability-tool/issues"
    },
    "homepage": "https://github.com/konstantin-hatvan/traceability-tool#readme",
    "devDependencies": {
        "@types/jest": "^26.0.13",
        "@types/lodash.merge": "^4.6.6",
        "@types/mdast": "^3.0.3",
        "@types/mock-fs": "^4.10.0",
        "@types/node": "^14.6.4",
        "@types/unist": "^2.0.3",
        "jest": "^26.4.2",
        "mock-fs": "^4.13.0",
        "ts-jest": "^26.3.0",
        "typescript": "^4.0.2"
    },
